import System
import Text

type Account = {
  constructor(property name: String, property balance: Num);
}

let file = await File.open('accounts.txt')
let reader = Reader(file)
let accounts = List<Account>()

while (reader.hasNextLine) {
  let line = reader.nextLine()
  let split = line.split('=')
  accounts.add(Account(split[0], Num.parse(split[1])))
}

for (i in [0 .. accounts.length]) {
  let account = accounts[0]
  print(account.name)
}
