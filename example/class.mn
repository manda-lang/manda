with IO

class Bird {
    new(property wingspan : Int);

    clone() => Bird(wingspan)

    fly() : void => stdout.writeln("I have a wingspan of %{wingspan}!")

    private doSomething() : void {
        // Do something here...
    }
}

fn birdOrInt (x: Bird | Int) : Int {
    return match x with {
        is Bird => x.wingspan
        is Int => x
    }
}

fn main() : void {
    var bird = Bird()
    bird.clone.fly()
}